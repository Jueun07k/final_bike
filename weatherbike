# 따릉이 데이터 날짜 컬럼은 이미 datetime.date 형식이라고 가정
df_bike['대여일시'] = pd.to_datetime(df_bike['대여일시'])
df_bike['날짜'] = df_bike['대여일시'].dt.date  # 날짜만 추출

# 날씨 데이터도 날짜 컬럼만 있다고 가정 (datetime.date 형식 혹은 문자열)
df_weather['날짜'] = pd.to_datetime(df_weather['날짜']).dt.date

# 날짜 기준으로 병합
weatherbike = pd.merge(df_bike, df_weather, how='left', on='날짜')

print(weatherbike.head())
print(weatherbike.shape)
